{
  "name": "fleet-monorepo-ts",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "fleet-monorepo-ts",
      "workspaces": [
        "apps/*",
        "packages/*",
        "backend"
      ]
    },
    "apps/admin": {
      "name": "@apps/admin",
      "version": "0.0.1"
    },
    "apps/customer": {
      "name": "@apps/customer",
      "version": "0.0.1",
      "license": "ISC"
    },
    "apps/mobile": {
      "name": "@apps/mobile",
      "version": "0.0.1"
    },
    "backend": {
      "version": "0.0.1"
    },
    "node_modules/@apps/admin": {
      "resolved": "apps/admin",
      "link": true
    },
    "node_modules/@apps/customer": {
      "resolved": "apps/customer",
      "link": true
    },
    "node_modules/@apps/mobile": {
      "resolved": "apps/mobile",
      "link": true
    },
    "node_modules/@packages/api-client": {
      "resolved": "packages/api-client",
      "link": true
    },
    "node_modules/@packages/ui": {
      "resolved": "packages/ui",
      "link": true
    },
    "node_modules/@packages/utils": {
      "resolved": "packages/utils",
      "link": true
    },
    "node_modules/backend": {
      "resolved": "backend",
      "link": true
    },
    "packages/api-client": {
      "name": "@packages/api-client",
      "version": "0.0.1"
    },
    "packages/ui": {
      "name": "@packages/ui",
      "version": "0.0.1"
    },
    "packages/utils": {
      "name": "@packages/utils",
      "version": "0.0.1"
    }
  }
}
